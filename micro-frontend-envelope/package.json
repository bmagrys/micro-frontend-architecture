{
  "name": "micro-frontend-envelope",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --extraWebpackConfig webpack.extra.js",
    "start:prod": "ng serve --aot --prod=true --vendor-chunk=false --optimization=true --extraWebpackConfig webpack.extra.js",
    "build": "ng build --prod --extraWebpackConfig webpack.bundle.extra.js",
    "build:aws": "ng build --prod --extraWebpackConfig webpack.bundle.aws.extra.js",
    "copy-bundles": "node copy-bundles.js",
    "copy-wc-polyfills": "node copy-wc-polyfills.js",
    "bundle": "npm run copy-bundles && npm run copy-wc-polyfills && npm run build",
    "bundle:aws": "npm run copy-bundles && npm run copy-wc-polyfills && npm run build:aws",
    "server": "http-server dist -p 8200",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/elements": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "core-js": "^3.1.4",
    "jquery": "^3.4.1",
    "ng2-charts": "^2.3.0",
    "popper.js": "^1.15.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "copy": "^0.3.2",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ngx-build-plus": "^8.0.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
